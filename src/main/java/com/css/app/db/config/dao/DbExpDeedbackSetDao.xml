<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.app.db.config.dao.DbExpDeedbackSetDao">

	<select id="queryObject" resultType="com.css.app.db.config.entity.DbExpDeedbackSet">
		select * from DB_EXP_DEEDBACK_SET where ID = #{value}
	</select>

	<select id="queryList" resultType="com.css.app.db.config.entity.DbExpDeedbackSet">
		select * from DB_EXP_DEEDBACK_SET 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from DB_EXP_DEEDBACK_SET 
	</select>
	 
	<insert id="save" parameterType="com.css.app.db.config.entity.DbExpDeedbackSet">
		insert into DB_EXP_DEEDBACK_SET
		(
			"ID", 
			"EXP_NAME", 
			"CREATED_USER", 
			"CREATED_USER_ID", 
			"CREATED_TIME", 
			"MODIFIED_USER", 
			"MODIFIED_USER_ID", 
			"MODIFIED_TIME", 
			"REMARK", 
			"FLAG"
		)
		values
		(
			#{id}, 
			#{expName}, 
			#{createdUser}, 
			#{createdUserId}, 
			#{createdTime}, 
			#{modifiedUser}, 
			#{modifiedUserId}, 
			#{modifiedTime}, 
			#{remark}, 
			#{flag}
		)
	</insert>
	 
	<update id="update" parameterType="com.css.app.db.config.entity.DbExpDeedbackSet">
		update DB_EXP_DEEDBACK_SET 
		<set>
			<if test="expName != null">"EXP_NAME" = #{expName}, </if>
			<if test="createdUser != null">"CREATED_USER" = #{createdUser}, </if>
			<if test="createdUserId != null">"CREATED_USER_ID" = #{createdUserId}, </if>
			<if test="createdTime != null">"CREATED_TIME" = #{createdTime}, </if>
			<if test="modifiedUser != null">"MODIFIED_USER" = #{modifiedUser}, </if>
			<if test="modifiedUserId != null">"MODIFIED_USER_ID" = #{modifiedUserId}, </if>
			<if test="modifiedTime != null">"MODIFIED_TIME" = #{modifiedTime}, </if>
			<if test="remark != null">"REMARK" = #{remark}, </if>
			<if test="flag != null">"FLAG" = #{flag}</if>
		</set>
		where ID = #{id}
	</update>
	
	<delete id="delete">
		delete from DB_EXP_DEEDBACK_SET where ID = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from DB_EXP_DEEDBACK_SET where ID in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>